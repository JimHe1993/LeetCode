class Solution:
    def sortColors(self, nums):
        """
        :type nums: List[int]
        :rtype: void Do not return anything, modify nums in-place instead.
        """
        if not nums:
            return
        # 3路快排——[0...l) == 0, [l...m) == 1, [m...r)未探索, [r...len(nums)) == 2
        l = 0
        m = 0
        r = len(nums)
        while m < r:
            if nums[m] == 1:
                m += 1
            elif nums[m] < 1:
                nums[l], nums[m] = nums[m], nums[l]
                l += 1
                m += 1
            else:
                nums[r-1], nums[m] = nums[m], nums[r-1]
                r -= 1
            
