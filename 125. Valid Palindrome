class Solution:
    def isPalindrome(self, s):
        """
        :type s: str
        :rtype: bool
        """
        if not s:
            return True
        # 解法一：遍历比较
        # s_list = []
        # for i in range(len(s)):
        #     if s[i].isalnum():
        #         s_list.append(s[i].lower())
        # return s_list == s_list[::-1]
            
        # 解法二：对撞指针
        i = 0
        j = len(s) - 1
        while i <= j:
            while i <= j and not s[i].isalnum():
                i += 1
            while i <= j and not s[j].isalnum():
                j -= 1
            if i > j:
                break
            if s[i].lower() != s[j].lower():
                return False
            i += 1
            j -= 1
        return True
