class Solution:
    def combinationSum3(self, k, n):
        """
        :type k: int
        :type n: int
        :rtype: List[List[int]]
        """
        res = []
        if not k or not n:
            return res
        self.__combine_sum_3(k, n, 1, res, [])
        return res
    # æœªå‰ªæ
    def __combine_sum_3(self, k, n, start, res, temp):
        if sum(temp) == n and len(temp) == k:
            res.append(temp)
            return
        if sum(temp) > n:
            return
        if len(temp) >= k:
            return
        for num in range(start, 10):
            self.__combine_sum_3(k, n, num+1, res, temp+[num])
        return
