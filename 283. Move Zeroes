class Solution:
    def moveZeroes(self, nums):
        """
        :type nums: List[int]
        :rtype: void Do not return anything, modify nums in-place instead.
        """
        if not nums:
            return
        # 思路一：先放非零元素，再赋值0
        # i = j = 0
        # while j < len(nums):
        #     if nums[j] != 0:
        #         nums[i] = nums[j]
        #         i += 1
        #     j += 1
        # while i < j:
        #     nums[i] = 0
        #     i += 1
        
        # 一边比较，一边交换。[0...k)是非零元素，[k...i)是0
        k = 0
        i = 0
        while i < len(nums):
            while i < len(nums) and nums[i] == 0:
                i += 1
            if i == len(nums):
                break
            if i == k:
                k += 1
                i += 1
            else:
                nums[k], nums[i] = nums[i], nums[k]
                k += 1
                i += 1
