class Solution:
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        if not nums:
            return []
        dict_c = {}
        for idx, num in enumerate(nums):
            if target - num in dict_c:
                return [dict_c[target - num], idx]
            else:
                dict_c[num] = idx
        return []
