class Solution:
    def reverseBetween(self, head, m, n):
        """
        :type head: ListNode
        :type m: int
        :type n: int
        :rtype: ListNode
        """
        dummy_head = ListNode(-1)
        dummy_head.next = head
        cur = dummy_head
        for _ in range(1, m):
            cur = cur.next
        pre = cur
        last = cur.next
        front = None
        for _ in range(m, n+1):
            cur = pre.next
            pre.next = cur.next
            cur.next = front
            front = cur
        cur = pre.next
        pre.next = front
        last.next = cur
        return dummy_head.next
        
