class Solution:
    def isAnagram(self, s, t):
        """
        :type s: str
        :type t: str
        :rtype: bool
        """
        if not s or not t:
            return True
        dict_s = {}
        for ss in s:
            if ss not in dict_s:
                dict_s[ss] = 1
            else:
                dict_s[ss] += 1
        for tt in t:
            if tt in dict_s:
                dict_s[tt] -= 1
                if dict_s[tt] == 0:
                    del dict_s[tt]
            else:
                return False
        if not dict_s:
            return True
        else:
            return False
